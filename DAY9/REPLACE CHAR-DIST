def replace_chars(s):
    freq = {char: s.count(char) for char in s}
    
    def replace_char(c):
        if c.isalpha():
            base = ord('A') if c.isupper() else ord('a')
            new_char = chr((ord(c.lower()) - ord('a') + freq[c]) % 26 + ord('a'))
            return new_char.upper() if c.isupper() else new_char
        return c
    
    return ''.join(replace_char(c) for c in s)

input_string = "ghee"
print("Modified string:", replace_chars(input_string))

OUTPUT:
Modified string: higg
