def is_palindrome(s):
    return s == s[::-1]
def palindromic_substrings(input_string):
    for i in range(len(input_string)):
        for j in range(i+1, len(input_string)+1):
            substring = input_string[i:j]
            if is_palindrome(substring):
                print(substring)
 
input_str = "ababa"
palindromic_substrings(input_str)

OUTPUT:
a
aba
ababa
b
bab
a
aba
b
a
