def print_and_sum_diagonal(matrix):
    diagonal_sum = 0
    for i in range(len(matrix)):
        print(matrix[i][i], end=" ")  
        diagonal_sum += matrix[i][i] 
    print("\nSum of diagonal elements:", diagonal_sum)
      
matrix = [[1, 2, 3],
          [4, 5, 6],
          [7, 8, 9]
  OUTPUT:
diagonal_elements: 1 5 9 
Sum of diagonal elements: 15
      
